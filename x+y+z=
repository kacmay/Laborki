#include <stdio.h>
#include <stdlib.h>

int sum1(int a, int b,int c)
{
    int d = a + b + c;
    return d;
}

int main()
{
    int x,y,z,s;
    s=0;
    printf("enter x: \n");
    scanf("%i",&x);
    printf("enter y: \n");
    scanf("%i",&y);
    printf("enter z: \n");
    scanf("%i",&z);
    s=sum1(x,y,z);
    printf("x+y+z=%i\n",s);
    return 0;
}
